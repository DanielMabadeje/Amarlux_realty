{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 250
    },
    "date": {
      "type": "date"
    },
    "isFeatured": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "Thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "blogImages": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "blogContent": {
      "type": "blocks",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true
    }
  }
}
